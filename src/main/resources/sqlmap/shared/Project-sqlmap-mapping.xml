<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
        "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap>


    <resultMap id="RM.Project" class="com.manage.book.dal.dataobject.ProjectDO">
        <result property="reportDate" column="reportDate" javaType="java.lang.String" jdbcType="VARCHAR"  />
        <result property="project" column="project" javaType="java.lang.String" jdbcType="VARCHAR"  />
        <result property="sellnum" column="sellnum" javaType="java.lang.Double" jdbcType="DOUBLE"  />
        <result property="amount" column="amount" javaType="java.lang.Double" jdbcType="DOUBLE"  />
        <result property="area" column="area" javaType="java.lang.Double" jdbcType="DOUBLE"  />
        <result property="avgprice" column="avgprice" javaType="java.lang.Double" jdbcType="DOUBLE"  />
    </resultMap>


    <sql id="Project.columns">
        reportDate, project,sellnum, amount, area, avgprice
    </sql>




    <delete id="dcs.Project.deleteByDate" parameterClass="java.util.Map">
        DELETE FROM project WHERE
        reportDate = #reportDate# and url = #url#
    </delete>




    <insert id="dcs.Project.insert">
        INSERT INTO project (
        reportDate ,project ,region,url,sellnum ,amount ,area ,avgprice
        ) VALUES (#reportDate#,#project#,#region#,#url#,#sellnum#,#amount#,#area#,#avgprice#)
    </insert>

    <select id="dcs.Project.queryByProject" resultMap="RM.Project" >
        SELECT
        /*dcs.Project.queryByDateAndProject*/ 		    <include refid="Project.columns"/>
        FROM project
        WHERE project = #project#
        order by reportDate
    </select>










</sqlMap>

